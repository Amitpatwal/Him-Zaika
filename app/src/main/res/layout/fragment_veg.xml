        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.68" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="207dp"
        android:layout_height="26dp"
        android:layout_marginTop="50dp"
